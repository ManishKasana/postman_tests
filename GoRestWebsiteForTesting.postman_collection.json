			"name": "Create a user",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var name = pm.variables.replaceIn(\"{{$randomFullName}}\");\r",
							"pm.variables.set(\"Name\", name);\r",
							"\r",
							"var email = pm.variables.replaceIn(\"{{$randomEmail}}\");\r",
							"pm.variables.set(\"Email\", email);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"const json_response = pm.response.json();\r",
							"pm.test(\"verify the valid name and email\", function(){\r",
							"    //pm.expect(pm.response.json().name).to.eql(\"pm.variables.get(\"Name\")\");\r",
							"    pm.expect(pm.response.json().name).to.eql(pm.variables.get(\"name\"));\r",
							"     //pm.expect(cookieHeader).to.include(\"HttpOnly\");\r",
							"});\r",
							"pm.collectionVariables.set(\"id\", json_response.id);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
